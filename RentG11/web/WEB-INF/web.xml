    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/Registro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reserva</servlet-name>
        <url-pattern>/Reserva</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>busquedaCliente</servlet-name>
        <url-pattern>/busquedaCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>busquedaRSMatricula</servlet-name>
        <url-pattern>/busquedaRSMatricula</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaRSMatricula</servlet-name>
        <url-pattern>/BusquedaRSMatricula</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusquedaRSFecha</servlet-name>
        <url-pattern>/BusquedaRSFecha</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
