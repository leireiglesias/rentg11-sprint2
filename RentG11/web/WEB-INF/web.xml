    <servlet>
        <servlet-name>busquedaRSMatricula</servlet-name>
        <servlet-class>servlets.BusquedaRSEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusquedaRSMatricula</servlet-name>
        <servlet-class>servlets.BusquedaRSMatricula</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusquedaRSFecha</servlet-name>
        <servlet-class>servlets.BusquedaRSFecha</servlet-class>
